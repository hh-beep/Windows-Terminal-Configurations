{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [          
        
        {
          "foreground": "#f9e2af",
          "style": "diamond",
          "template": "  [ ",
          "type": "text"
        },
        {
          "foreground": "#cdd6f4",
          "style": "diamond",
          "template": " \uf120",
          "type": "shell"
        },
        {
          "foreground": "#cdd6f4",
          "style": "diamond",
          "type": "text",
          "template": "\u2800"
        },
        {
          "foreground": "#cdd6f4",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "  \ue5ff  {{ .Path }}",
          "type": "path"
        },




        





        {
          "foreground": "#cdd6f4",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "  {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} <#f9e2af>]</>",
          "type": "git",

          "trailing_diamond": "\ue0b4",
          "leading_diamond": "\u2500\u2500\ue0b6"
        }



        
      ],
      "type": "prompt"
    },

    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#74c7ec",
          "style": "plain",
          "template": "  ~>",
          "type": "text"
        },
        

        {
          "foreground": "#f2f2f2",
          "foreground_templates": [
            "{{ if gt .Code 0 }}{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}


